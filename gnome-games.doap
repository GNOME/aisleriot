      <foaf:name>Thomas H.P. Andersen</foaf:name>
      <foaf:mbox rdf:resource="mailto:phomes@gmail.com" />
      <gnome:userid>thomashpa</gnome:userid>
    </foaf:Person>
  </maintainer>
  <maintainer>
